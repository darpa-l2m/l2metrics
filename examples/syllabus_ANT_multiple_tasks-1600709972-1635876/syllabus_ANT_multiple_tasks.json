{
    "name": "ANT Subtype A syllabus with MinigridKit episodes",
    "author": "Megan Baker <megan.baker@jhuapl.edu>",
    "contains": "episodes",
    "default_params": {},
    "instructions": [
      {"$phase":  "0.test", "enable_updates" : false},
      {"$repeat": { "$episode": "$minigridkit:MinigridLava1", "agent_pos": [1,1]}, "count": 25},
      {"$repeat": { "$episode": "$minigridkit:Minigrid16x16", "agent_pos": [1,1]}, "count": 25},

      {"$phase":  "1.train", "enable_updates" : true},
      {"$repeat": { "$episode": "$minigridkit:MinigridLava1", "agent_pos": [1,1]}, "count": 250},

      {"$phase":  "1.test", "enable_updates" : false},
      {"$repeat": { "$episode": "$minigridkit:Minigrid16x16", "agent_pos": [1,1]}, "count": 25},
      {"$repeat": { "$episode": "$minigridkit:MinigridLava1", "agent_pos": [1,1]}, "count": 25},


      {"$phase":  "2.train", "enable_updates" : true},
      {"$repeat": { "$episode": "$minigridkit:Minigrid16x16", "agent_pos": [1,1]}, "count": 250},

      {"$phase":  "2.test", "enable_updates" : false},
      {"$repeat": { "$episode": "$minigridkit:MinigridLava1", "agent_pos": [1,1]}, "count": 25},
      {"$repeat": { "$episode": "$minigridkit:Minigrid16x16", "agent_pos": [1,1]}, "count": 25}
    ]
  }
  
